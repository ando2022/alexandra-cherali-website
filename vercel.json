{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "vite",
  "installCommand": "npm ci",
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/admin/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

